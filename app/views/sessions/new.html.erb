<h1> Login </h1>

<%= form_tag sessions_path do %>
  <%= text_field_tag :name, '',  placeholder: 'Login with your name' %>
    <%= password_field_tag :password, '',  placeholder: 'enter password' %>
  <%= button_tag :submit %><br><br>
<%end %>

<button><%= link_to "Sign up", new_user_path %></button>
